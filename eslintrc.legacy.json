/* ESLint configuration for bigbrainmaths.com and other legacy ES5 projects */

{
	"extends": [
		"./eslintrc.es5.json"
	],

	"rules": {
		"require-jsdoc": 0,
		"no-inline-comments": 0,
		"line-comment-position": 0,
		"sort-keys": 0,
		"consistent-this": 0,
		"key-spacing": 0,
		"no-extra-parens": 0,
		"vars-on-top": 0,
		"id-length": 0,
		"no-mixed-requires": 0,
		"newline-per-chained-call": 0,
		"object-curly-newline": 0,
		"no-param-reassign": 0,
		"no-proto": 0,
		"no-alert": 0,
		"consistent-return": 0,
		"no-restricted-properties": 0,
		"new-cap": 0,
		"no-underscore-dangle": 0
	}
}
